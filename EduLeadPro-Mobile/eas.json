{
    "cli": {
        "version": ">= 5.0.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_API_URL": "http://192.168.29.16:5000/api/mobile",
                "EXPO_PUBLIC_WS_URL": "ws://192.168.29.16:5000"
            }
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "https://eduleadconnect.vercel.app/api/mobile",
                "EXPO_PUBLIC_WS_URL": "wss://eduleadconnect.vercel.app"
            }
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "EXPO_PUBLIC_API_URL": "https://eduleadconnect.vercel.app/api/mobile",
                "EXPO_PUBLIC_WS_URL": "wss://eduleadconnect.vercel.app"
            }
        }
    },
    "submit": {
        "production": {}
    }
